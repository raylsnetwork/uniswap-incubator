{
  "name": "suitability-zk",
  "version": "1.0.0",
  "description": "Zero-Knowledge Suitability Assessment System",
  "main": "index.js",
  "scripts": {
    "compile": "circom circuits/SuitabilityAssessment.circom --r1cs --wasm --sym --c",
    "setup": "node scripts/setup.js",
    "contribute": "snarkjs zkey contribute circuits/SuitabilityAssessment_0000.zkey circuits/SuitabilityAssessment_final.zkey",
    "export-verifier": "snarkjs zkey export verificationkey circuits/SuitabilityAssessment_final.zkey circuits/verification_key.json",
    "generate-proof": "node scripts/generateProof.js",
    "mock-proof": "node scripts/mockProof.js",
    "verify-proof": "snarkjs groth16 verify circuits/verification_key.json scripts/public.json scripts/proof.json",
    "test": "node test/SuitabilityAssessment.test.js",
    "integrate": "node scripts/integrateWithFrontend.js"
  },
  "dependencies": {
    "circomlib": "^2.0.5",
    "snarkjs": "^0.7.5"
  },
  "devDependencies": {
    "jest": "^29.5.0"
  },
  "keywords": [
    "zero-knowledge",
    "suitability",
    "circom",
    "snarkjs",
    "ethereum"
  ],
  "author": "Your Name",
  "license": "MIT"
}
